{
  "name": "types",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/types",
        "main": "libs/types/src/index.ts",
        "tsConfig": "libs/types/tsconfig.lib.json",
        "assets": ["libs/types/*.md"]
      }
    },
    "buildcjs": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/types/cjs.in",
        "main": "libs/types/src/index.ts",
        "tsConfig": "libs/types/tsconfig.lib.json",
        "rootDir": "."
      }
    },
    "buildes2020": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/types/es2020.in",
        "main": "libs/types/src/index.ts",
        "tsConfig": "libs/types/tsconfig.es2020.json"
      }
    },
    "buildes2015": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/types/es2015.in",
        "main": "libs/types/src/index.ts",
        "tsConfig": "libs/types/tsconfig.es2015.json"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs types {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/types/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/types/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
